{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "prettier": true,
    "interface-name": false,
    "no-duplicate-imports": false,
    "object-literal-sort-keys": false,
    "no-submodule-imports": false
  }
}