{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**"]
  },
  "rules": {
    "prettier": false,
    "interface-name": false,
    "no-duplicate-imports": false,
    "quotemark": true
  }
}
